{
  "builds": [
    {
      "src": "pepe-clean/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "pepe-clean/dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/pepe-clean/dist/$1"
    }
  ]
}